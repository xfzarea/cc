  height: 34rpx;
  animation: toBig 2.5s ease-in-out infinite ;
}
.fontToBig{
  animation: fontToBig 2.5s ease-in-out infinite ;
}
@keyframes toBig{
  0% {width: 144rpx;height: 34rpx;}
  60% {width: 144rpx;height: 34rpx;}
  70% {width: 160rpx;height: 38rpx;}
  80% {width: 144rpx;height: 34rpx;}
  90% {width: 160rpx;height: 38rpx;}
  100% {width: 144rpx;height: 34rpx;}
}
@keyframes fontToBig{
  0% {font-size: 28rpx;}
  60% {font-size: 28rpx;}
  70% {font-size: 30rpx;}
  80% {font-size: 28rpx;}
  90% {font-size: 30rpx;}
  100% {font-size: 28rpx;}
}